{
    "manifest_version": 3,
    "name": "URL Specific Extension",
    "version": "1.0",
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "declarativeContent",
        "storage"
    ],
    "host_permissions": [
        "https://www.youtube.com/*",
        "https://www.teamblind.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }